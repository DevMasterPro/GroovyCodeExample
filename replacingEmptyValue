def result

def CIs ="[,,][ciname2,cilocation2,cmts2][ciname3,cilocation3,]"
def engineerList ="[eng1,engID1,Phone1,group1][eng2,engID2,Phone2,group2]"




def parseStringToArray(String arrayString){

    //replacing empty value with null
    def replaceEmptyValue = arrayString.replaceAll(",]",",Null]").replaceAll("\\[,","[Null,").replaceAll(",,",",Null,")
   //replacing the first square bracket
    arrayString = replaceEmptyValue.replaceAll("\\[",'')
    replacing
    def array = arrayString.split("]");

    println(array);

    def arrayList = new ArrayList();

    //loop array of ciname1,cilocation1,CMTS1
    array.each{
        //split by , to ciname1, cilocation1, CMTS
        def subArray = it.split(",")
      //  println(subArray)
        arrayList.add(subArray)
    }
    //println(arrayList);

    return arrayList
}


//parse cis
def ciList = parseStringToArray(CIs)
def engList = parseStringToArray(engineerList)


//print


engList.each{ eng->
    ciList.each{ ci->


        println(ci[0].toString().trim())
        println(ci[1].toString().trim())
        println(ci[2].toString().trim())
        println(eng[0].toString().trim())
        println(eng[1].toString().trim())
        println(eng[2].toString().trim())
        println(eng[3].toString().trim())
        println("\n")

    }
}


